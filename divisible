using namespace std;


void main()
{
 int a,b,divi;
 cout<<"ingrese a:";
 cin>>a;
 cout<<"ingrese b:";
 cin>>b;
 if (a%b==0)
 {
	 divi=(a/b);
	 cout<< divi;
 }

  getch();
}

